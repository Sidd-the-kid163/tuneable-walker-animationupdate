  </link>
  <link name="RightThighUpper">
    <inertial>
      <origin xyz="0.0 0.0 -0.1" rpy= "0 0 0"/>
      <mass value="1.7"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0.006" iyz="0" izz="0"/>
    </inertial>
  </link>
  <link name="RightThighLower">
    <inertial>
