  </link>


  <!-- Joint Definitions -->
  <joint name="LeftHip" type="revolute">
    <parent link="Torso"/>
    <child link="LeftThighUpper"/>
    <origin xyz="0 0 -0.4999" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="400.0" lower="-1.6" upper="1.6" velocity="4"/>
  </joint>
  <joint name="LeftThighExtension" type="prismatic">
    <axis xyz="0 0 -1"/>
    <limit effort="1000.0" lower="-0.4" upper="0.4" velocity="1000"/>
    <origin rpy="0 0 0" xyz="0 0 -0.2"/>
    <parent link="LeftThighUpper"/>
    <child link="LeftThighLower"/>
  </joint>
  <joint name="LeftKnee" type="revolute">
    <parent link="LeftThighLower"/>
    <child link="LeftShinUpper"/>
    <origin xyz="0.0 0.0 -0.2" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit effort="400.0" lower="0.1" upper="1.5" velocity="4"/>
  </joint>
  <joint name="LeftShinExtension" type="prismatic">
    <axis xyz="0 0 -1"/>
    <limit effort="1000.0" lower="-0.4" upper="0.4" velocity="1000"/>
    <origin rpy="0 0 0" xyz="0 0 -0.2"/>
    <parent link="LeftShinUpper"/>
    <child link="LeftShinLower"/>
  </joint>
    <joint name="RightHip" type="revolute">
