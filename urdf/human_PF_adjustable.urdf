  <!-- Linkage Definitions -->
  <link name="Torso">
    <inertial>
      <origin xyz="0 0 -0.28507"/>
      <mass value="13"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0.1940" iyz="0" izz="0"/>
    </inertial>
  </link>
  <link name="LeftThighUpper">
